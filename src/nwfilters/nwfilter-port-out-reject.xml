<filter name='port-out-reject' chain='ipv4' priority='-700'>
  <!-- Accept a list of outward port ranges for every IP -->
  <rule action='reject' direction='out' prority='500'>
    <all match='yes' dstipaddr='$PORT_OUT_REJECT_IP[@1]' dstportstart='$PORT_OUT_REJECT_START[@2]' dstportend='$PORT_OUT_REJECT_END[@2]'/>
  </rule>
</filter>

